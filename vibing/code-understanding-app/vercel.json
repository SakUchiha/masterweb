{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.js/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/frontend/favicon.ico"
    },
    {
      "src": "/favicon-dark.ico",
      "dest": "/frontend/favicon-dark.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/frontend/manifest.json"
    },
    {
      "src": "/css/(.*)",
      "dest": "/frontend/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/frontend/js/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/frontend/images/$1"
    },
    {
      "src": "/data/(.*)",
      "dest": "/frontend/data/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "backend/server.js/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "backend/server.js/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
