{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/favicon.ico",
      "dest": "/frontend/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/frontend/manifest.json"
    },
    {
      "src": "/sw.js",
      "dest": "/frontend/sw.js"
    },
    {
      "src": "/css/(.*)",
      "dest": "/frontend/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/frontend/js/$1"
    },
    {
      "src": "/data/(.*)",
      "dest": "/frontend/data/$1"
    },
    {
      "src": "/",
      "dest": "/frontend/index.html"
    },
    {
      "src": "/lessons",
      "dest": "/frontend/lessons.html"
    },
    {
      "src": "/editor",
      "dest": "/frontend/editor.html"
    },
    {
      "src": "/ai",
      "dest": "/frontend/ai.html"
    },
    {
      "src": "/study-guide",
      "dest": "/frontend/study-guide.html"
    },
    {
      "src": "/lesson-viewer",
      "dest": "/frontend/lesson-viewer.html"
    },
    {
      "src": "/code-explainer",
      "dest": "/frontend/code-explainer.html"
    },
    {
      "src": "/contact",
      "dest": "/frontend/contact.html"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "headers": [
    {
      "source": "/frontend/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
