{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "frontend",
  "framework": null,
  "functions": {
    "api/index.js": {
      "maxDuration": 15
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.js"
    },
    {
      "source": "/lessons",
      "destination": "lessons.html"
    },
    {
      "source": "/editor",
      "destination": "editor.html"
    },
    {
      "source": "/ai",
      "destination": "ai.html"
    },
    {
      "source": "/ask-ai",
      "destination": "ask-ai.html"
    },
    {
      "source": "/code-explainer",
      "destination": "code-explainer.html"
    },
    {
      "source": "/contact",
      "destination": "contact.html"
    },
    {
      "source": "/lesson-viewer",
      "destination": "lesson-viewer.html"
    },
    {
      "source": "/study-guide",
      "destination": "study-guide.html"
    },
    {
      "source": "/cache-clear",
      "destination": "cache-clear.html"
    },
    {
      "source": "/(.*)",
      "destination": "index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, must-revalidate"
        }
      ]
    }
  ],
  "regions": ["sin1"]
}
