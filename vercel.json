{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "vibing/code-understanding-app/frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "vibing/code-understanding-app/frontend/favicon.ico"
    },
    {
      "src": "/(css|js|images)/(.*)",
      "dest": "vibing/code-understanding-app/frontend/$1/$2"
    },
    {
      "src": "/(.*)\\.(?:html|json|ico|png)$",
      "dest": "vibing/code-understanding-app/frontend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "vibing/code-understanding-app/frontend/index.html"
    }
  ]
}