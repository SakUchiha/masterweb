{
  "version": 2,
  "buildCommand": "cd code-understanding-app && npm install && npm run build",
  "outputDirectory": "code-understanding-app/dist",
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/code-understanding-app/dist/$1" }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "true"
  }
}
